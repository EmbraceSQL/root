{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "EmbraceSQL",
  "compilerOptions": {
    "target": "ESNext",
    "allowImportingTsExtensions": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "noEmit": true,
    "strict": true,
    "jsx": "react-jsxdev",
    "sourceMap": true,
    "skipLibCheck": true,
    "esModuleInterop": true
  },
  "exclude": ["node_modules", "dist"]
}
