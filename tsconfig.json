{
  "compilerOptions": {
    "target": "ESNext",
    "sourceMap": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "paths": {},
    "lib": ["ESNext"],
    "noEmit": true,
    "typeRoots": ["./node_modules/@types"],
    "jsx": "react-jsxdev"
  },
  "exclude": ["node_modules", "dist"]
}
